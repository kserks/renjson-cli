#!/usr/bin/env node
const pkg = require('../package');
process.title = "vnjson@"+require('../package').version;
var argv = require('minimist')(process.argv.slice(2));
const
  index                         = require('../index');



var key = argv._[0];

if(key==='init'){
  index.init()
}else if(key==="run"){
  index.run();
}else{
  console.log('Uknown paramm')
}

